{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_APP": "main.py"
  },
  "functions": {
    "main.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "outputDirectory": "dist"
}
